<PropertyList>
  <nasal>
    <load>
      var livery_update = aircraft.livery_update.new("Aircraft/Dash8Q200/Models", 10);

      var self = cmdarg();

      var float_0 = self.getNode("sim/multiplay/generic/float[0]", 1);
      var float_1 = self.getNode("sim/multiplay/generic/float[1]", 1);
      
      var cabin_door = self.getNode("sim/model/door-positions/cabin/position-norm", 1);
      var cargo_door = self.getNode("sim/model/door-positions/cargo/position-norm", 1);

      cabin_door.alias(float_0);
      cargo_door.alias(float_1);
    </load>

    <unload>
      livery_update.stop();
      cabin_door.unalias();
      cargo_door.unalias();
    </unload>
  </nasal>
